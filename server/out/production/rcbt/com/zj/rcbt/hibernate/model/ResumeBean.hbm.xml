<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zj.rcbt.hibernate.model.ResumeBean" table="resume" schema="rcbt">
        <id name="resumeId">
            <column name="resume_id" sql-type="int(11)"/>
        </id>
        <property name="timeScape">
            <column name="time_scape" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="company">
            <column name="company" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="department">
            <column name="department" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="post">
            <column name="post" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="idNum">
            <column name="id_num" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>

    </class>
</hibernate-mapping>